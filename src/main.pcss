* {
    box-sizing: border-box;
    font-family: 'Inter', sans-serif;
}

body, ul, li, h1, h2, h3, h4, h5, h6, p {
    margin: 0;
    padding: 0;
}
body {
    background: #151718;
    color: #EAE9EC
}
ul, li {
    display: block;
}
a, a:hover, a:active, a:focus {
    text-decoration:none;
    outline: none;
}
input, button {
    border: none;
}
input:hover, input:focus, button:focus {
    outline: none;
}